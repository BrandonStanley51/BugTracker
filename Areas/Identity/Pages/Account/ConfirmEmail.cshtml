@page
@model ConfirmEmailModel
@{
    Layout = "_LoginLayout";
    ViewData["Title"] = "Your All Set! Your Email Has Been Confirmed";
}

<h1>@ViewData["Title"]</h1>
<br />
<div class="card">

    <div class="card-body">
        <p>@Model.StatusMessage</p>

        <a class="btn btn-primary btn-lg btn-block" asp-area="Identity" asp-page="/Account/Login"><i class="fa fa-sign-in"></i><span> SIGN IN</span></a>

    </div>
</div>
